{
  "name": "weave-md",
  "version": "0.0.1-alpha.0",
  "description": "Markdown flavor for inline, expandable references",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "test": "vitest run",
    "test:validate": "vitest run conformance.validate",
    "test:parse": "vitest run conformance.parse"
  },
  "keywords": [
    "markdown",
    "weave",
    "references"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weavepage/weave-md.git"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
