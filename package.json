{
  "name": "weave-md",
  "version": "0.0.1-alpha.0",
  "description": "Markdown flavor for inline, expandable references",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "test": "vitest run",
    "test:validate": "vitest run conformance.validate",
    "test:parse": "vitest run conformance.parse"
  },
  "keywords": [
    "markdown",
    "weave",
    "references"
  ],
  "author": "weavepage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weavepage/weave-md.git"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@weave-md/basic": "workspace:*",
    "@weave-md/core": "workspace:*",
    "@weave-md/parse": "workspace:*",
    "@weave-md/validate": "workspace:*",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
