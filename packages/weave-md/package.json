{
  "name": "weave-md",
  "version": "0.5.0",
  "description": "Weave Markdown - inline, expandable references for markdown. Umbrella package for @weave-md/* packages.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "keywords": [
    "weave",
    "markdown",
    "references",
    "inline",
    "expandable"
  ],
  "author": "weavepage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weavepage/weave-md.git",
    "directory": "packages/weave-md"
  },
  "dependencies": {
    "@weave-md/core": "workspace:^",
    "@weave-md/parse": "workspace:^",
    "@weave-md/validate": "workspace:^",
    "@weave-md/basic": "workspace:^"
  }
}
